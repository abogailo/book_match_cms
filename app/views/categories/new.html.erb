<div class="categories new">
  <h2>Create category</h2>

  <%= form_for(@category) do |f| %>

    <table summary="category form fields">
      <tr>
        <th>Name</th>
        <td><%= f.text_field(:name) %></td>
      </tr>
      
</table>
    <div class="form-buttons">
      <%= f.submit("Create category") %>
    </div>

  <% end %>
</div>